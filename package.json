  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "electron": "^9.4.4",
    "electron-builder": "^22.13.1",
    "electron-packager": "^15.4.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "mocha": "^8.4.0",
    "node-abi": "^2.30.1",
    "spectron": "^11.1.0"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "chromedriver": "^84.0.1",
    "dmx": "^0.2.5",
    "electron-first-run": "^3.0.0",
    "fs-extra": "^9.1.0",
    "howler": "^2.2.3",
    "nanotimer": "^0.3.15",
